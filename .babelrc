{
    "presets": [
    [
        "@babel/preset-env",
        {
        "targets": {
            "node": "10"
        }
        }
    ],
    "@babel/preset-react"
    ],
    "plugins": [
        [
        "@babel/plugin-transform-runtime",
        {
            "absoluteRuntime": false,
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": false
        }
        ],
        ["@babel/plugin-proposal-optional-chaining"]
    ]
}

